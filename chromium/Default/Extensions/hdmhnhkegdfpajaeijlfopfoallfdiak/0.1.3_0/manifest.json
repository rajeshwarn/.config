{
   "author": "Richard Neomy",
   "background": {
      "scripts": [ "main.js" ]
   },
   "browser_action": {
      "default_icon": {
         "16": "data/icons/16.png",
         "32": "data/icons/32.png"
      },
      "default_popup": "data/popup/index.html"
   },
   "description": "A panel to access all your installed applications and their settings",
   "homepage_url": "http://add0n.com/apps-launcher.html",
   "icons": {
      "128": "data/icons/128.png",
      "16": "data/icons/16.png",
      "48": "data/icons/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Apps Launcher for Chrome",
   "permissions": [ "storage", "tabs", "management", "notifications", "contextMenus", "chrome://favicon/" ],
   "short_name": "ialfchrome",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1.3"
}
