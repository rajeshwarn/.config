{
   "background": {
      "persistent": false,
      "scripts": [ "media/js/piktab/analytics.js", "media/js/piktab/background.js" ]
   },
   "browser_action": {
      "default_icon": "media/img/icons/piktab.png",
      "default_title": "Piktab"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.piktab.com/ https://ssl.google-analytics.com https://secure.statcounter.com/ https://suggestqueries.google.com/ https://www.google.com https://apis.google.com https://www.gstatic.com https://connect.facebook.net https://media.freepik.com/min/g/auth_js ; object-src 'self'",
   "description": "Discover free graphic resources in your Chrome Tab every day",
   "homepage_url": "http://www.piktab.com/",
   "icons": {
      "128": "media/img/icons/piktab.png",
      "16": "media/img/icons/piktab.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Piktab",
   "optional_permissions": [ "chrome://favicon/", "cookies", "topSites" ],
   "permissions": [ "management", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.19",
   "web_accessible_resources": [ "*" ]
}
