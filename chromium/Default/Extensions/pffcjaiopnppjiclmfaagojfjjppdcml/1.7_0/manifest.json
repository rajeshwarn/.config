{
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "icon.png"
      },
      "default_popup": "popup.html"
   },
   "default_locale": "en",
   "description": "A Material Design of Chrome Download Manager.",
   "icons": {
      "128": "icon.png",
      "16": "icon16.png",
      "24": "icon24.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "64": "icon64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Material Design Download Manager",
   "offline_enabled": true,
   "optional_permissions": [ "management" ],
   "permissions": [ "downloads", "downloads.open", "downloads.shelf", "clipboardWrite", "clipboardRead", "notifications", "activeTab" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.7"
}
