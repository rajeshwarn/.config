{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "16.png",
      "default_popup": "menu.html"
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://ajax.googleapis.com https://api.bing.com https://www.google-analytics.com; object-src 'self'",
   "description": "Help support your favorite free content publishers online.",
   "icons": {
      "128": "128.png",
      "16": "16.png",
      "48": "48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Support Free Content",
   "offline_enabled": false,
   "permissions": [ "tabs", "http://*/*", "https://*/*", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.15.9"
}
