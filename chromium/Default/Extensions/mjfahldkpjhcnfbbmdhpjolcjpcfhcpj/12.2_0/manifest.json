{
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "icon19.png",
         "38": "icon38.png"
      },
      "default_popup": "popup.html",
      "default_title": "Display Menu"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content-id.js" ],
      "matches": [ "https://*/*", "http://*/*" ],
      "run_at": "document_idle"
   }, {
      "all_frames": true,
      "js": [ "content-st.js" ],
      "matches": [ "https://*/*", "http://*/*" ],
      "run_at": "document_start"
   } ],
   "description": "Customize New Tab Page & More!",
   "homepage_url": "http://sermak.ca/PTHelp.pdf",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "New Tab Start Page Pro",
   "options_page": "options.html",
   "permissions": [ "webRequest", "webRequestBlocking", "webNavigation", "tabs", "contextMenus", "storage", "https://*/*", "http://*/*" ],
   "short_name": "Tab/Start Pro",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "12.2"
}
